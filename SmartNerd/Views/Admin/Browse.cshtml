@model SmartNerd.Models.Admin.BrowsePage
<div class="col-sm-2">
    <div class="sideCategory">
        <h3>Categories</h3>
        <ol>
            @foreach(var cat in Model.Categories)
            {
                <li><a href="@Url.Action("Browse", "Admin", new {categoryID = cat.CategoryID})">@cat.CategoryName</a></li>
            }
        </ol>
    </div>
</div>
<div class="col-sm-10">
    <h2>Index</h2>
    <div id="menuListing" class="row">
        @foreach(var prod in Model.Products) {
            <div class="col-sm-4 col-md-3"><a href="@Url.Action("Product", "Admin", new { productID = prod.ProductID })"><img src="@SmartNerd.Classes.Util.ProductUrl(prod.ProductID)" /><span>@prod.ProductName</span></a></div>
        }
    </div>
</div>